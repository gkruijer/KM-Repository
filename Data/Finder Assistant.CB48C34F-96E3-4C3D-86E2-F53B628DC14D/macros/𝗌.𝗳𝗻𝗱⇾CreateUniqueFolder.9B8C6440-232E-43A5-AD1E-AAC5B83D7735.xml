						<key>Text</key>
						<string>-- Requires Keyboard Maestro 8.0.3+set kmInst to system attribute "KMINSTANCE"tell application "Keyboard Maestro Engine"	set kmParentPathPosixPath to getvariable "local__ParentPath" instance kmInst	set kmName to getvariable "local__Name" instance kmInstend tell-- Expand tilde (~) Paths
tell application "System Events"
	set theParentPathPosixPathEpanded to POSIX path of disk item kmParentPathPosixPath
end tell

-- Convert path to colon-delimited
set parent_path to POSIX file theParentPathPosixPathEpanded as alias
try	tell application "Finder"		set folder_list to get the name of every folder of parent_path	end tellon error	set folder_list to {}end tryset new_folder to kmNameset n to 1repeat	if new_folder is in folder_list then		set new_folder to kmName &amp; " " &amp; n		set n to n + 1	else		exit repeat	end ifend repeattry	tell application "Finder"		activate		make new folder at folder parent_path with properties {name:new_folder}	end tell	return new_folderon error	return ""end try</string>
