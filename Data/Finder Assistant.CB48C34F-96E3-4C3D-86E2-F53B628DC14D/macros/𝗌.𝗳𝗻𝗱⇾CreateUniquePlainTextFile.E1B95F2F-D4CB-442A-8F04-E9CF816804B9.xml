						<key>Text</key>
						<string>-- Requires Keyboard Maestro 8.0.3+set kmInst to system attribute "KMINSTANCE"tell application "Keyboard Maestro Engine"	set kmParentPathPosixPath to getvariable "local__ParentPath" instance kmInst	set kmBaseName to getvariable "local__BaseName" instance kmInst	set kmExtension to getvariable "local__Extension" instance kmInstend tell-- Expand tilde (~) Pathstell application "System Events"	set theParentPathPosixPathEpanded to POSIX path of disk item kmParentPathPosixPathend tell-- Convert path to colon-delimited
set parent_path to POSIX file theParentPathPosixPathEpanded as aliastry	tell application "Finder"		set file_list to get the name of every file of parent_path whose name ends with ("." &amp; kmExtension)	end tellon error	set file_list to {}end tryset new_file to kmBaseName &amp; "." &amp; kmExtensionset n to 1repeat	if new_file is in file_list then		set new_file to kmBaseName &amp; " " &amp; n &amp; "." &amp; kmExtension		set n to n + 1	else		exit repeat	end ifend repeattry	tell application "Finder"		activate		make new file at folder parent_path with properties {name:new_file}	end tell	return new_fileon error	return ""end try</string>
