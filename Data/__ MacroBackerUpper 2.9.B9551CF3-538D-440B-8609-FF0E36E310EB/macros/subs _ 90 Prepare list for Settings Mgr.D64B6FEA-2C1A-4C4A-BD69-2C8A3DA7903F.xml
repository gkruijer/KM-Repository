				<string>#!/usr/bin/perl

use strict;
use warnings;

# Input variable
my $input = $ENV{KMVAR_local_TheList};

# Remove trailing newline, if any
chomp($input);

# Replace line breaks with pipes
$input =~ s/\n/|/g;

# Print the modified string
print "$input\n";
</string>
